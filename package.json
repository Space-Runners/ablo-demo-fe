{
  "name": "ablo-demo-fe",
  "description": "A tool that lets you put AI generated designs on templates like clothes, with a full fledged text and image editor",
  "version": "0.0.3",
  "type": "module",
  "license": "MIT",
  "homepage": "web.ablo.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/Space-Runners/ablo-demo-fe"
  },
  "bugs": {
    "url": "https://github.com/Space-Runners/ablo-demo-fe"
  },
  "keywords": [
    "AI",
    "image",
    "editor",
    "design"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:library": "tsc && vite build --config vite-lib.config.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "postinstall": "cd node_modules/fabric && node build.js modules=ALL requirejs fast exclude=gestures,accessors",
    "preview": "vite preview"
  },
  "main": "./dist/ablo-demo-fe.js",
  "module": "./dist/ablo-demo-fe.js",
  "exports": {
    ".": {
      "import": "./dist/ablo-demo-fe.js",
      "require": "./dist/ablo-demo-fe.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/bangers": "^5.0.3",
    "@fontsource/karla": "^5.0.3",
    "@fontsource/koulen": "^5.0.3",
    "@fontsource/luckiest-guy": "^5.0.3",
    "@fontsource/poppins": "^5.0.3",
    "@fontsource/press-start-2p": "^5.0.3",
    "@fontsource/roboto": "^5.0.3",
    "@fontsource/roboto-condensed": "^5.0.3",
    "@fontsource/silkscreen": "^5.0.3",
    "@fontsource/space-grotesk": "^5.0.3",
    "@fontsource/special-elite": "^5.0.3",
    "@react-oauth/google": "^0.11.0",
    "@tanstack/react-query": "^4.29.5",
    "axios": "^1.4.0",
    "fabric": "^5.3.0",
    "framer-motion": "^10.16.4",
    "html-to-image": "^1.11.11",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3",
    "react-dropzone": "^12.0.4",
    "react-icons": "^4.3.1",
    "react-is": "^18.0.0",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.5",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.7.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
